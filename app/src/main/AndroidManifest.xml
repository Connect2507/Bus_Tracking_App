<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="enroute.pallavi.chugh.bus_tracking_app" >

    <uses-sdk android:minSdkVersion="11" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="com.google.android.providers.gsf.permission.READ_GSERVICES" />
    <!--
 The ACCESS_COARSE/FINE_LOCATION permissions are not required to use
         Google Maps Android API v2, but are recommended.
    -->
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <uses-feature
        android:name="android.hardware.camera"
        android:required="false" />

    <uses-permission android:name="android.permission.MANAGE_DOCUMENTS" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />


    <application
        android:name="enroute.pallavi.chugh.bus_tracking_app.App"
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.AdminPage"
            android:label="@string/title_activity_admin_page" >

        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.Route_info"
            android:label="@string/title_activity_route_info"
            android:parentActivityName=".AdminPage"
             >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.parse_check"
            android:label="@string/title_activity_parse_check"
            android:parentActivityName=".Route_info"
            android:launchMode="singleTop" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.mediator"
            android:label="@string/title_activity_mediator" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.live_view"
            android:label="@string/title_activity_live_view" >
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <!--<meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="@string/google_maps_key"></meta-data>-->
        <meta-data
            android:name="com.google.android.maps.v2.API_KEY"
            android:value="AIzaSyB2mAg5K1h-FrhuC77jQioLTO7CS5bCICk"></meta-data>

        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.MapsActivity"
            android:label="@string/title_activity_maps" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.studentinfoedit"
            android:label="@string/title_activity_studentinfoedit"
            android:parentActivityName="enroute.pallavi.chugh.bus_tracking_app.parse_check" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.Google_Map_Upload"
            android:label="Google_Map_Upload" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.startpage"
            android:label="@string/title_activity_startpage" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.teacherinfoedit"
            android:label="@string/title_activity_teacherinfoedit" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.editteacher"
            android:label="@string/title_activity_editteacher" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.addteacher"
            android:label="add_teacher" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.parentactivity"
            android:label="parentactivity" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.parentpageone"
            android:label="Bus_Tracking_App" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.parentmapfull"
            android:label="parentmapfull" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.addstudent"
            android:label="@string/title_activity_addstudent"
            android:parentActivityName="enroute.pallavi.chugh.bus_tracking_app.AdminPage" >
        </activity>
        <activity
            android:name="enroute.pallavi.chugh.bus_tracking_app.Parent_first_page"
            android:label="@string/title_activity_parent_first_page"
            android:parentActivityName="enroute.pallavi.chugh.bus_tracking_app.startpage">
        </activity>
    </application>

</manifest>
